//Practical 2
public class Palindrome {
 public static void main(String[] args) {
 int digit, num, rev, temp;
 num = Integer.parseInt(args[0]);
 temp = num;
 rev = 0;
 while (num != 0) {
 digit = num % 10;
 rev = rev * 10 + digit;
 num = num / 10;
 }
 if (temp == rev) {
 System.out.println("The " + rev + " number
is Palindrome");
 } else {
 System.out.println("The " + temp + " is not
a Palindrome");
 }
 }
